{
  "name": "gulp-html-toc",
  "description": "Gulp plugin for html-toc, for generating a HTML table of contents.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/gulp-html-toc",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-html-toc",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-html-toc/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "html-toc": "^1.0.0",
    "plugin-error": "^0.1.2",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.11",
    "mocha": "^3.2.0",
    "vinyl": "^2.0.1"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "html",
    "toc"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "html-toc",
        "gulp-prettify",
        "gulp-htmlmin"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
