{
  "name": "gulp-html-toc",
  "description": "Gulp plugin for html-toc, for generating a HTML table of contents.",
  "version": "1.1.0",
  "homepage": "https://github.com/jonschlinkert/gulp-html-toc",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/gulp-html-toc",
  "bugs": {
    "url": "https://github.com/jonschlinkert/gulp-html-toc/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "html-toc": "^2.0.0",
    "plugin-error": "^0.1.2",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "gulp-format-md": "^0.1.12",
    "mocha": "^3.4.1",
    "vinyl": "^2.0.2"
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "html",
    "html-toc",
    "table-of-contents",
    "toc"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "gulp-htmlmin",
        "gulp-prettify",
        "html-toc"
      ]
    },
    "lint": {
      "reflinks": true
    }
  }
}
